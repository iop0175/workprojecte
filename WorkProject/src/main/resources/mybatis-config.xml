<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias type="kr.ac.workproject.model.Work" alias="work" />
		<typeAlias type="kr.ac.workproject.model.Mydate"
			alias="mydate" />
		<typeAlias type="kr.ac.workproject.model.Company"
			alias="company" />
		<typeAlias type="kr.ac.workproject.model.Contract"
			alias="contract" />
		<typeAlias type="kr.ac.workproject.model.Vendor"
			alias="vendor" />
		<typeAlias type="kr.ac.workproject.model.Signcom"
			alias="signcom" />
		<typeAlias type="kr.ac.workproject.model.Private"
			alias="private" />
	</typeAliases>

	<mappers>
		<mapper resource="mybatis/work.xml" />
		<mapper resource="mybatis/mydate.xml" />
		<mapper resource="mybatis/company.xml" />
		<mapper resource="mybatis/contract.xml" />
		<mapper resource="mybatis/private.xml" />
	</mappers>

</configuration>